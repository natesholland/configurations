// Place your key bindings in this file to overwrite the defaults
[
    { "key": "cmd+l",       "command": "workbench.action.nextEditor" },
    { "key": "cmd+h",       "command": "workbench.action.previousEditor" },
    { "key": "cmd+j",       "command": "workbench.action.terminal.focus"},
    { "key": "cmd+j",       "command": "workbench.action.focusActiveEditorGroup", "when": "terminalFocus"},
    { "key": "cmd+shift+j", "command": "workbench.action.toggleMaximizedPanel" },
    { "key": "cmd+ctrl+j",  "command": "workbench.action.togglePanel" },
    { "key": "alt+l",  "command": "workbench.action.terminal.focusNext"},
    { "key": "alt+h",  "command": "workbench.action.terminal.focusPrevious"}
]
